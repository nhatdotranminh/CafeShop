<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="~/Content/images/favicon.ico">
    <link rel="shortcut icon" href="~/Content/images/favicon.ico" />
    @Styles.Render("~/Content/css")
    
    @*
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Content/js/jquery-migrate-1.1.1.js"></script>
    <script src="~/Content/js/jquery.equalheights.js"></script>
    <script src="~/Content/js/jquery.ui.totop.js"></script>
    <script src="~/Content/js/jquery.easing.1.3.js"></script>
        *@
    <script src="~/Scripts/jquery.js"></script>
    @Scripts.Render("~/bundles/uiToTop")
    <script src="~/Scripts/jquery.tabs.min.js"></script>
  
    @Scripts.Render("~/bundles/jquerymigrate")
    @Scripts.Render("~/bundles/jqueryequalheights")
    @Scripts.Render("~/bundles/jqueryeasing")
    @Scripts.Render("~/bundles/tab")
    @Scripts.Render("~/bundles/touchTouch")
    
    <script type="text/javascript">
        $(document).ready(function(){

          $().UItoTop({ easingType: 'easeOutQuart' });
         $('.gallery a.gal').touchTouch();

       })
    </script>
    <title>@ViewBag.Title - My ASP.NET Application</title>
  
   

</head>
<body class="">
    <!--=====================================================Header=============================================================-->
    <header>
        <div class="container_12">
           
            <div class="grid_12">
                <div class="socials">
                    <a href="#"></a>
                    <a href="#"></a>
                    <a href="#"> </a>
                    <a href="#" class="last"></a>
                  
                </div>
               
              
                <h1>
                    <a href="index.html">
                        <img src="~/Content/images/logo.png" alt="Boo House">
                    </a>
                </h1>
                <div class="menu_block">


                    <nav id="bt-menu" class="bt-menu">
                        <a href="#" class="bt-menu-trigger"><span>Menu</span></a>
                        <ul>
                            <li class="bt-icon "><a href="/Home/Index">Home</a></li>
                            <li class="bt-icon"><a href="/Home/About">About</a></li>
                            <li class="current bt-icon"><a href="/Category/Index">Menu</a></li>
                            <li class="bt-icon"><a href="">Blog</a></li>
                            <li class="bt-icon"><a href="">Reservation</a></li>
                            <li class="bt-icon"><a href="/Home/Contact">Contacts</a></li>
                        </ul>
                    </nav>

                    <div class="clear"></div>
                </div>
                <div class="clear"></div>
            </div>
        </div>
    </header>
    <!--========================s==============================Body===================================================================-->
    <div class="content">
      
        <div class="container_12">
            @RenderBody()
            <hr />

        </div>
       
        </div>
    <!--=================================================Footer ========================================================================================================-->
    <footer>
        <div class="container_12">
            <div class="grid_6 prefix_3">
                <a href="index.html" class="f_logo"><img src="~/Content/images/f_logo.png" alt=""></a>

            </div>
        </div>
    </footer>

    <script>
      $(document).ready(function(){
         $(".bt-menu-trigger").toggle(
          function(){
            $('.bt-menu').addClass('bt-menu-open');
          },
          function(){
            $('.bt-menu').removeClass('bt-menu-open');
          }
        );
      })
    </script>
   



    
    @*@Scripts.Render("~/bundles/jquery")*@
    
   @RenderSection("scripts", required: false)
   
       
</body>
</html>
